@model IEnumerable<MarketPlace.TraderMaster>

@{
    ViewBag.Title = "TraderList";
}

<h2>TraderList</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TraderCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TraderName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TraderRegisterNo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TraderCommission)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CreateDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UpdateDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserMaster.UserFirstName)
        </th>
        <th>Action</th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TraderCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TraderName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TraderRegisterNo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TraderCommission)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreateDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UpdateDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserMaster.UserFirstName)
        </td>
        <td>
            @Html.ActionLink("Edit", "EditTrader", new { id=item.TraderID })
        </td>
    </tr>
}

</table>
